{
  "name": "@saastral/shared",
  "version": "0.1.0",
  "description": "SaaStral Shared - Shared utilities and types",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
